<div class="uk-section uk-section-light" id="team">
    <div class="uk-container uk-text-center">
        <h2 class="uk-heading-mini text-light uk-margin-medium-bottom">Our Team</h2>

        <div class="uk-grid-large" uk-grid>
            {% for i in range(0,10) %}
                <div class="uk-width-1-3 uk-width-1-5@m">
                    <img uk-img="data-src: assets/images/avatar.jpg" alt="Achilles Saxby" class="uk-border-circle">
                    <div class="uk-text-center uk-margin-small-top">
                        <div class="text-bold">Achilles Saxby</div>
                        <div class="text-light uk-text-small">Principal Data Scientist</div>
                        <div class="text-light uk-text-small">The Dreamer</div>
                    </div>
                </div>
            {% endfor %}        
        </div>

        <div class="uk-padding-small uk-margin-large-top">
            <div class="uk-button uk-button-primary">Join Us</div>
        </div>
    </div>
</div>