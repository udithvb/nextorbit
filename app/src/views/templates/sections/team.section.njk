<div class="uk-section uk-section-light" id="team">
    <div class="uk-container uk-text-center">
        <h2 class="uk-heading-small text-light uk-margin-medium-bottom">Our Team</h2>

        <div class="uk-grid-small" uk-grid>
            {% for i in range(0,10) %}
                <div class="uk-width-1-2 uk-width-1-5@m">
                    <div class="uk-placeholder">
                        <i class="fas fa-image"></i>
                    </div>
                </div>
            {% endfor %}        
        </div>

        <div class="uk-padding-small uk-margin-large-top">
            <div class="uk-button uk-button-primary">Join Us</div>
        </div>
    </div>
</div>